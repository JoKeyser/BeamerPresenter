[General]
cache%20pages=-1
gui%20config="@ABS_GUI_CONFIG_PATH@"
manual="@ABS_DOC_PATH@"
memory=1.0486e+08
frame%20time=50
gestures=true

[drawing]
history%20length%20hidden=20
history%20length%20visible=50
mode=cumulative

[keys]
Ctrl%2BO=open
Ctrl%2BS=save
Ctrl%2BShift%2BS=save as
Ctrl%2BY=redo left, redo right, redo
Ctrl%2BZ=undo left, undo right, undo
Ctrl%2BQ=quit
F11=fullscreen
PgDown=next
PgUp=previous
Shift%2BPgDown=next skipping overlays
Shift%2BPgUp=previous skipping overlays
Shift%2BC=clear, clear left, clear right
End=last
Home=first
P=toggle timer
Space=update

[rendering]
page%20part%20threshold=2.5
renderer=mupdf
rendering%20command=/usr/bin/mutool
rendering%20arguments=draw,-F%format,-w%width,-h%height,-o-,%file,%page
